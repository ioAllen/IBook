<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.moduleBookMall">

    <application
        android:name="com.common.base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/book_mall_app_name"
        android:supportsRtl="true"
        android:theme="@style/public_AppTheme">

        <activity
            android:name="com.moduleBookMall.ui.launch.BookMallLaunchActivity"
            android:theme="@style/public_AppTheme_NoActionBar_Transparent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.moduleBookMall.ui.bookMall.BookMallActivity"
            android:screenOrientation="portrait" />
    </application>
</manifest>
