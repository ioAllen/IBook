<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.iBook">

    <application
        android:name="com.common.base.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/public_AppTheme">
        <activity
            android:name=".ui.launch.LaunchActivity"
            android:theme="@style/public_AppTheme_NoActionBar_Transparent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.base.MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/public_AppTheme_NoActionBar" />
    </application>

</manifest>